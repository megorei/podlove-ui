{
  "name": "@podlove/player",
  "version": "5.7.4",
  "description": "HTML5 Goodness for Podcasting",
  "homepage": "https://github.com/podlove/ui/tree/development/apps/player",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run build:dependencies && webpack serve --progress --config .build/webpack.config.dev.js",
    "build:dependencies": "lerna run build --scope @podlove/components --stream",
    "build:dev": "npm run build:dependencies && webpack --config .build/webpack.config.dev.js",
    "clean": "rm -rf dist publish",
    "build": "npm run clean && npm run build:dependencies && webpack --config .build/webpack.config.prod.js",
    "postpublish": "npm run clean",
    "publish:prepare": "npm run clean && npm run build && podlove-prepublish",
    "integration:build": "npm run clean && npm run build:dependencies && webpack --config .build/webpack.config.dev.js",
    "integration:server": "http-server -p 9000 dist/ > http.log 2>&1 &",
    "integration:ci": "cypress run --browser chrome",
    "integration:dev": "cypress open --browser chrome"
  },
  "keywords": [],
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "license": "MIT",
  "dependencies": {
    "@podlove/button-actions": "^5.7.4",
    "@podlove/components": "^5.7.4",
    "@podlove/player-actions": "^5.7.4",
    "@podlove/player-config": "^5.7.4",
    "@podlove/player-sagas": "^5.7.4",
    "@podlove/player-state": "^5.7.4",
    "@podlove/utils": "^5.7.4",
    "color": "3.1.2",
    "copy-to-clipboard": "3.2.0",
    "core-js": "3.15.2",
    "date-fns": "2.16.1",
    "normalize-scss": "7.0.1",
    "ramda": "0.27.1",
    "redux": "4.0.4",
    "redux-vuex": "0.3.2",
    "regenerator-runtime": "0.13.3",
    "v-tooltip": "2.0.2",
    "vue": "2.6.14",
    "vue-i18n": "8.15.0"
  },
  "devDependencies": {
    "@podlove/build": "^5.7.4",
    "cypress": "5.1.0",
    "http-server": "0.11.1",
    "webpack": "5.45.1",
    "webpack-dev-server": "3.11.2"
  }
}
